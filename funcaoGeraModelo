.data
stringTeste:	.asciiz	"Ronaldo"
numeroTeste:	.asciiz	"23345555666445654"
stringModelo:	.asciiz

.text
la	$a0,numeroTeste
la	$a1,stringTeste
jal	gerarStringModelo
move	$a0,$v0
li	$v0,4
syscall
li	$v0,10
syscall


gerarStringModelo:	#gera a string no formato (X,Y)
move	$t0,$a0		#endereco do primeiro item da string de numero X	
move 	$t1,$a1		#endereco do primeiro item da string Y
la	$t3,stringModelo
li	$t4,'('		
sb	$t4,($t3)
addi	$sp,$sp,-4	#abre espaco na pilha pra guardar o valor de $ra
sw	$ra,($sp)
addi	$t3,$t3,1
move	$a1,$t3		#passa como parametro o valor do endereco aonde armazenar	
move	$a0,$t0		#passa como parametro o que armazenar - no caso, endereco do primeiro item da string de numero X
jal	saveString
li	$t4,','
sb	$t4,($t3)
addi	$t3,$t3,1
move	$a0,$t1
move	$a1,$t3
jal	saveString
li	$t4,')'
sb	$t4,($t3)
lw	$ra,($sp)
addi	$sp,$sp,4
la	$v0,stringModelo
jr	$ra


saveString:		#$a0 o que armazenar e $a1 recebe aonde armazenar  
move	$t0,$zero
move	$t6,$a0		#$t6 tem o endereco do que salvar
move	$t3,$a1		#$t5 tem o endereco de onde armazenar
for1:	lb	$t2, ($t6)
	beq	$zero,$t2,endFor1	#se o valor de t2 == \0 sai do loop
		sb	$t2,($t3)
		addi	$t3,$t3,1
		addi	$t6,$t6,1
		addi	$t0,$t0,1
		j	for1
endFor1:
	jr	$ra	








